{
  "FileFilter": {
    "IncludedExtensions": [
      ".cs",
      ".csproj",
      ".sln",
      ".json",
      ".md",
      ".xaml",
      ".xaml.cs"
    ],
    "ExcludedDirectories": [
      "**/bin/**",
      "**/obj/**",
      "**/.vs/**",
      "**/.git/**"
    ],
    "MaxFileSizeMb": 16
  },
  "Chunking": {
    "MaxChunkSizeKb": 128
  },
  "Output": {
    "OutputDirectory": "./ai-output"
  }
}
